{
  "name": "timelite",
  "description": "String date and time utilities",
  "repository": "joseluisq/timelite",
  "version": "0.0.9",
  "license": "MIT",
  "author": {
    "name": "Jos√© Luis Quintana",
    "url": "http://git.io/joseluisq"
  },
  "files": [
    "README.md",
    "LICENSE.md"
  ],
  "keywords": [
    "time",
    "time string",
    "format",
    "add times",
    "subtraction times",
    "time utilities",
    "date",
    "date utilities"
  ],
  "main": "index.js",
  "devDependencies": {
    "@types/jasmine": "^2.8.7",
    "@types/node": "^10.0.3",
    "jasmine": "^3.0.0",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.0.0",
    "karma-spec-reporter": "^0.0.32",
    "karma-typescript": "^3.0.12",
    "rollup": "^0.58.2",
    "rollup-plugin-typescript2": "^0.13.0",
    "tslint": "^5.9.1",
    "tslint-config-standard-plus": "^1.0.0",
    "typescript": "^2.8.3",
    "uglify-js": "^3.3.23"
  },
  "scripts": {
    "version": "npm run build",
    "build": "npm run test && npm run clean && npm run compile && npm run minify:browser",
    "compile": "npm run compile:browser && npm run compile:cjs",
    "compile:cjs": "tsc --module commonjs --target es5",
    "compile:browser": "rollup -c && npm run minify:browser",
    "minify:browser": "uglifyjs ${npm_package_name}.umd.js -cm --source-map -o ${npm_package_name}.umd.min.js",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "npm run lint && karma start --single-run --reporters spec",
    "test:watch": "karma start",
    "lint": "tslint --format stylish --project tsconfig.json"
  }
}
